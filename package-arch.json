{
  "name": "wishlist",
  "version": "1.0.0",
  "description": "A tool for creating, organizing, and sharing wish lists for the Holidays, birthdays, and more!",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch": "concurrently --names \"webpack, sass\" --prefix name \"npm run start\" \"npm run styles:watch\"",
    "styles": "sass src/scss/main.scss public/css/main.css",
    "styles:watch": "npm run styles -- --watch",
    "server": "nodemon ./start.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arobillard/wishlist.git"
  },
  "keywords": [
    "reactjs",
    "nodejs",
    "wishlist",
    "gifts",
    "app",
    "material",
    "android"
  ],
  "author": "Adam Robillard",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/system": "^4.9.14",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "0.15.3",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "deep-diff": "^1.0.2",
    "dotenv": "^8.2.0",
    "es6-promisify": "^6.1.1",
    "express": "^4.17.1",
    "express-session": "1.15.1",
    "express-validator": "3.1.2",
    "moment": "2.17.1",
    "mongodb": "^3.6.2",
    "mongoose": "5.1.0",
    "mongoose-mongodb-errors": "0.0.2",
    "nodemailer": "3.1.5",
    "nodemon": "^1.18.3",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "4.0.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "sass": "^1.27.0",
    "slugify": "^1.4.5",
    "validator": "^13.1.17",
    "whatwg-fetch": "^3.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "concurrently": "^5.3.0"
  },
  "bugs": {
    "url": "https://github.com/arobillard/wishlist/issues"
  },
  "homepage": "https://github.com/arobillard/wishlist#readme",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
